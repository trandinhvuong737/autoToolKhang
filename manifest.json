{
  "manifest_version": 3,
  "name": "Link Batch Processor",
  "version": "1.0",
  "permissions": [
    "tabs", 
    "storage", 
    "scripting"
  ],
  "host_permissions": [
    "https://*/*", 
    "http://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon16.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}